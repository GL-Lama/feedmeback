<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="database.SubscribeModule.SubscribeModule" table="subscribeModule">
		<meta attribute="class-description">
			This class contains the module detail.
		</meta>
		<id name="idSm" type="int" column="idSm">
			<generator class="native"/>
		</id>
		<property name="idTeacher" type="int" column="idTeacher"/>
		<property name="idModule" type="int" column="idModule"/>
	</class>
</hibernate-mapping>