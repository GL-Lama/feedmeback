<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="database.Question.Question" table="question">
		<meta attribute="class-description">
			This class contains the question detail.
		</meta>
		<id name="idQuestion" type="int" column="idQuestion">
			<generator class="native"/>
		</id>
		<property name="idForm" column="idForm" type="int"/>
		<property name="numQuestion" column="numQuestion" type="int"/>
		<property name="question" column="question" type="string"/>
		<property name="idType" column="idType" type="int"/>
	</class>
</hibernate-mapping>